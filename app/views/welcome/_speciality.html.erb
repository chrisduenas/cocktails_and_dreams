<%= form_for(resource, as: resource_name, url: user_registration_path(resource_name), html: {method: :patch}) do |f| %>
    <div class="types">
        <div class="type">
            <%= f.check_box :standard %>
            <%= f.label :standard %>
        </div>
        <div class="type">
            <%= f.check_box :flair %>
            <%= f.label :flair%>
        </div>
        <div class="type">
            <%= f.check_box :mixologist %>
            <%= f.label :mixologist%>
        </div>
    </div>
<% end %>